{
  "expo": {
    "name": "typedraw",
    "description": "Real Time Collaboration",
    "slug": "mobile",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "0.2.1",
    "orientation": "portrait",
    "icon": "./mobile/assets/icon.png",
    "splash": {
      "image": "./mobile/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "node_modules/react-navigation/src/**/*.png",
      "node_modules/react-navigation-stack/**/*.png"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
