{
  "extends": ["airbnb"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "implicit-arrow-linebreak": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/prop-types": "off",
    "arrow-parens": "off",
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off"
  }
}